{
  "name": "reactjs-s4y",
  "version": "1.0.3",
  "license": "MIT",
  "main": "././dist/index.js",
  "typings": "././dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react sticky"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "npm run build && changeset publish"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "hunghg255",
  "module": "./dist/reactjs-s4y.esm.js",
  "size-limit": [
    {
      "path": "./dist/reactjs-s4y.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "./dist/reactjs-s4y.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@size-limit/preset-small-lib": "^8.1.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "homepage": "https://github.com/hunghg255/reactjs-s4y",
  "repository": {
    "type": "git",
    "url": "https://github.com/hunghg255/reactjs-s4y"
  },
  "bugs": {
    "url": "https://github.com/hunghg255/reactjs-s4y/issues"
  }
}
